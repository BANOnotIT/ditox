  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "docs",
    "lib",
    "src",
    "LICENSE",
    "mod.ts",
    "README.md"
  ],
  "scripts": {
    "clean": "shx rm -rf dist lib",
    "test-deno": "deno test test-deno",
    "build": "rollup -c",
    "watch": "rollup -c --watch"
  }
